    <div id="outer-wrapper" class="index home">
      <div class="row" id="content-wrapper">
        <div class="clear"></div>
        <div class="clear"></div>
        <div id="main-wrapper">
          <div class="box-title"><h2>Latest Posts</h2></div>
          <div class="clear"></div>
          <div class="main section" id="main" name="Main Wrapper">
            <div class="widget Blog" data-version="1" id="Blog1">
              <div class="blog-posts hfeed">
                <% posts.forEach(post => { %>
                  <div class="post-outer">
                    <div class="post" data-file="<%= post.url %>" data-target="article">
                      <div class="post-header"></div>
                      <article class="my-wrapz">
                        <div class="post-detail">
                          <font class="retitle">
                            <h2 class="post-title entry-title">
                              <span>
                                <a href="/posts<%= post.url %>">
                                  <%= post.title %>
                                </a>
                              </span>
                            </h2>
                          </font>
                          <div class="date-header">
                            <div class="entry-meta">
                              <div class="meta-item meta-author">
                                By
                                <span class="vcard author">
                                  <span class="fn">
                                    <a class="g-profile" href="<%= post.authorUrl %>" rel="author">
                                      <%= post.author %>
                                    </a>
                                  </span>
                                </span>
                              </div>
                              <div class="meta-item meta-rtime">
                                <span class="eta"><%= post.readingTimeDisplay %></span>
                              </div>
                            </div>
                            <div class="post-date">
                              <span class="post-date-day"><%= post.date.day %></span>
                              <span class="post-date-month"><%= post.date.month %></span> -
                              <span class="post-date-year"><%= post.date.year %></span>
                            </div>
                            <div class="resumo">
                              <div id="summary<%= post.id %>" style="display: block">
                                  <% post.previewHTML.forEach((line) => { %> 
                                    <%- line %>
                                  <% }); %>
                              </div>
                            </div>
                            <div class="entry-footer">
                              <a class="typology-button" href="/posts<%= post.url %>">Read More</a>
                            </div>
                            <div style="clear: both"></div>
                          </div>
                        </div>
                      </article>
                    </div>
                  </div>
                <% }); %>
                
              </div>
              <div class="blog-pager" id="blog-pager">
                <span class="showpageOf">Page 1 of 3</span
                  ><span class="showpagePoint">1</span>
                <% for (let i=1; i < Math.ceil(options.POST_COUNT/options.POSTS_PER_PAGE); i++) { %> 
                  <span class="showpageNum">
                    <a href="/posts?page=<%=i+1%>&offset=<%=options.POSTS_PER_PAGE*i+1%>" ><%=i+1%></a>
                  </span>
                <% } %>

                <% if (options.POST_COUNT >= options.POST_PER_PAGE) { %>
                  <span class="showpageNum">
                    <a href="#">Next</a>
                  </span>
                <%} %>
                
              </div>
              <div class="clear"></div>
              <!--<div class="blog-feeds">
                <div class="feed-links">
                  Subscribe to:
                  <a
                    class="feed-link"
                    href="https://typography-themexpose.blogspot.com/feeds/posts/default"
                    target="_blank"
                    type="application/atom+xml"
                  >
                    Posts ( Atom )
                  </a>
                </div>
              </div>-->
            </div>
            <div class="widget HTML" data-version="1" id="HTML901"></div>
            <div class="widget HTML" data-version="1" id="HTML902"></div>
